<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="my.supercleanerapp.ui.fragments.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_services"
        android:name="my.supercleanerapp.ui.fragments.ServicesFragment"
        android:label="@string/title_services"
        tools:layout="@layout/fragment_services" />

    <fragment
        android:id="@+id/navigation_reservations"
        android:name="my.supercleanerapp.ui.fragments.ReservationsFragment"
        android:label="@string/title_reservations"
        tools:layout="@layout/fragment_reservations" />
    <fragment
        android:id="@+id/navigation_reserved_service"
        android:name="my.supercleanerapp.ui.fragments.ReservedServiceFragment"
        android:label="@string/title_reserved_services"
        tools:layout="@layout/fragment_reserved_service" />
</navigation>